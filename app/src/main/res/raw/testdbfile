CREATE TABLE "CashPayment" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `saleId` INTEGER, `amount` REAL, `currency_type` INTEGER,'lastUpdate' TEXT DEFAULT current_timestamp);
CREATE TABLE "CurrencyReturns" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `saleId` INTEGER, `amount` REAL, 'lastUpdate' TEXT DEFAULT current_timestamp,`currency_type` INTEGER, FOREIGN KEY(`saleId`) REFERENCES `sales.id` );
CREATE TABLE CurrencyType ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `type` TEXT );
CREATE TABLE Currencys ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT,`currencyCode` TEXT,`country`TEXT,`rate` INTEGER ,'lastUpdate' TEXT DEFAULT current_timestamp);
CREATE TABLE "Rule11" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `amount` INTEGER, `discountAmount` INTEGER, `contain` INTEGER, `club_contain` INTEGER );
CREATE TABLE "Rule3" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `percent` REAL, `contain` INTEGER );
CREATE TABLE "Rule5" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `giftID` INTEGER ,'product_id' INTEGER,'price'INTEGER);
CREATE TABLE "Rule7" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `price` REAL, `product_id` INTEGER, `contain_club` INTEGER );
CREATE TABLE "Rule8" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `percent` REAL, `product_id` INTEGER, `contain_club` INTEGER );
CREATE TABLE "_order" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `userOffer` REAL, `productId` INTEGER, `counter` INTEGER, `saleId` TEXT, `price` REAL, `original_price` REAL, `discount` REAL, `customerAssistantID` INTEGER, FOREIGN KEY(`productId`) REFERENCES `products.id`, FOREIGN KEY(`saleId`) REFERENCES `sales.id` );
CREATE TABLE a_report ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `lastUpdate` TEXT DEFAULT current_timestamp, `byUser` INTEGER, `amount` REAL, `lastSaleID` INTEGER , `lastZReportID` INTEGER , FOREIGN KEY(`byUser`) REFERENCES `users.id` );
CREATE TABLE `checks` ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `checkNum` INTEGER, `bankNum` INTEGER, `branchNum` INTEGER , `accountNum` INTEGER, `amount` REAL, `_date` TEXT DEFAULT current_timestamp, `deleted` INTEGER DEFAULT 0 , `saleId` INTEGER, FOREIGN KEY(`saleId`) REFERENCES `sales.id`);
CREATE TABLE City( id INTEGER PRIMARY KEY, name TEXT );
CREATE TABLE club ( `id` INTEGER PRIMARY KEY AUTOINCREMENT,`name` TEXT NOT NULL,'description' Text ,'type' INTEGER , 'percent' REAL DEFAULT 0 ,'amount' REAL DEFAULT 0, 'point' REAL DEFAULT 0 );
CREATE TABLE currency_operation ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `lastUpdate` TEXT DEFAULT current_timestamp, `operation_id` INTEGER, operation_type TEXT,`amount` REAL, `currency_type` INTEGER );
CREATE TABLE "custmerAssest" ( `order_id` INTEGER, `customerAssistantID` INTEGER, `amount` INTEGER, `type` INTEGER DEFAULT 0, `salesCase` TEXT, `id` INTEGER PRIMARY KEY AUTOINCREMENT );
CREATE TABLE `customer` ( `id` INTEGER, `name` TEXT NOT NULL, `birthday` TEXT NOT NULL, `gender` TEXT, `email` TEXT, `job` TEXT NOT NULL, `phoneNumber` TEXT, `address` TEXT NOT NULL, `hide` INTEGER DEFAULT 0, `city_id` INTEGER , `club_id` INTEGER , PRIMARY KEY(`id`) FOREIGN KEY(`city_id`) REFERENCES `city.id` FOREIGN KEY(`club_id`) REFERENCES `club.id` );
CREATE TABLE departments ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT NOT NULL UNIQUE, `creatingDate` TEXT NOT NULL DEFAULT current_timestamp, `byUser` INTEGER, `hide` INTEGER DEFAULT 0, FOREIGN KEY(`byUser`) REFERENCES `users.id` )
CREATE TABLE "idsCounter" ( `android_metadata` INTEGER, `sqlite_sequence` INTEGER, `departments` INTEGER, `order` BLOB, `payment` INTEGER, `_order` INTEGER, `userPermissions` INTEGER, `scheduleWorkers` INTEGER, `z_report` INTEGER, `checks` INTEGER, `a_report` INTEGER, `tbl_settings` INTEGER, `products` INTEGER, `city` INTEGER, `customer` INTEGER, `permissions` INTEGER, `users` INTEGER, `club` INTEGER, `sales` INTEGER, `sumPoint` INTEGER, `value_ofPoint` INTEGER, `productOffer` INTEGER, `offers` INTEGER, `Rule5` INTEGER, `usedPoint` INTEGER, `Rule11` INTEGER, `Rule7` INTEGER, `Rule8` INTEGER, `Rule3` INTEGER, `CashPayment` INTEGER, `currency_operation` INTEGER, `custmerAssest` INTEGER, `CurrencyReturns` INTEGER );
CREATE TABLE "offers" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT NOT NULL, `startDate` TEXT DEFAULT current_timestamp, `endDate` TEXT DEFAULT current_timestamp, `creatingDate` TEXT DEFAULT current_timestamp, `status` INTEGER DEFAULT 0, `byUser` INTEGER, `ruleName` INTEGER NOT NULL, `ruleID` INTEGER NOT NULL );
CREATE TABLE "payment" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `paymentWay` TEXT NOT NULL, `saleId` INTEGER, `amount` REAL, FOREIGN KEY(`saleId`) REFERENCES `sales.id` );
CREATE TABLE `permissions` ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT NOT NULL);
CREATE TABLE "productOffer" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `productId` INTEGER, `offerId` INTEGER, FOREIGN KEY(`offerId`) REFERENCES `offers.id` , FOREIGN KEY(`productId`) REFERENCES `products.id`);
CREATE TABLE "products" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT NOT NULL, `barcode` INTEGER NOT NULL, `description` TEXT, `price` REAL NOT NULL, `costPrice` REAL, `withTax` INTEGER NOT NULL DEFAULT 1, `weighable` INTEGER NOT NULL DEFAULT 0, `creatingDate` TEXT NOT NULL DEFAULT current_timestamp, `hide` INTEGER DEFAULT 0, `depId` INTEGER NOT NULL, `byUser` INTEGER NOT NULL, `status` INTEGER, with_pos INTEGER DEFAULT '1', with_point_system INTEGER DEFAULT '1', FOREIGN KEY(`depId`) REFERENCES `departments.id`, FOREIGN KEY(`byUser`) REFERENCES `users.id` );
CREATE TABLE "sales" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `byUser` INTEGER, `saleDate` TEXT DEFAULT current_timestamp, `replacementNote` INTEGER DEFAULT 0, `canceled` INTEGER DEFAULT 0, `totalPrice` REAL, `totalPaid` REAL, `custmer_id` INTEGER, `custmer_name` TEXT, FOREIGN KEY(`byUser`) REFERENCES `users.id` );
CREATE TABLE "scheduleWorkers" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `userId` INTEGER, `date` TEXT DEFAULT current_timestamp, `startTime` TEXT DEFAULT current_timestamp, `exitTime` TEXT );
CREATE TABLE "sumPoint" ( `sale_id` INTEGER, `point_amount` INTEGER, `custmer_id` INTEGER, FOREIGN KEY(`sale_id`) REFERENCES `sales.id` );
CREATE TABLE "usedPoint" ( `sale_id` INTEGER, `unUsed_point_amount` INTEGER, `custmer_id` INTEGER, FOREIGN KEY(`sale_id`) REFERENCES `sales.id` );
CREATE TABLE `userPermissions` ( `userId` INTEGER , `permissionId` INTEGER,FOREIGN KEY(`userId`) REFERENCES `users.id`,FOREIGN KEY(`permissionId`) REFERENCES `permissions.id`);
CREATE TABLE "users" ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `userName` TEXT UNIQUE, `firstName` TEXT NOT NULL, `lastName` TEXT, `visitDate` TEXT NOT NULL DEFAULT current_timestamp, `pwd` TEXT NOT NULL, `hide` INTEGER DEFAULT 0, `phoneNumber` TEXT, `present` REAL NOT NULL DEFAULT 0, `hourlyWage` REAL DEFAULT 0.0, `permissions_name` TEXT );
CREATE TABLE "value_ofPoint" ( `id` INTEGER, `value` INTEGER, `lastUpdate` TEXT DEFAULT current_timestamp, PRIMARY KEY(`id`) );
CREATE TABLE z_report ( `id` INTEGER PRIMARY KEY AUTOINCREMENT, `lastUpdate` TEXT DEFAULT current_timestamp, `byUser` INTEGER, `startSaleID` INTEGER, `endSaleID` INTEGER , FOREIGN KEY(`byUser`) REFERENCES `users.id`);
