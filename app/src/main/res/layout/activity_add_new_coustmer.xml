<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:padding="12dp"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/add_coustmer_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:background="@drawable/edit_text_search"
        android:layout_marginStart="18dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Name"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView55"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coustmer name"
        android:padding="10dp"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignTop="@+id/add_coustmer_name"
        />

    <EditText
        android:id="@+id/addCoustmer_Birthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/add_coustmer_name"
        android:layout_alignStart="@+id/add_coustmer_name"
        android:layout_below="@+id/add_coustmer_name"
        android:layout_marginTop="12dp"
        android:background="@drawable/edit_text_search"

        android:ems="10"
        android:hint="Date Of Birthday"
        android:inputType="date"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignTop="@+id/addCoustmer_Birthday"
        android:text="Date Of Birthday"
        android:layout_alignParentLeft="true"
        android:padding="12dp"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/addCoustmer_Birthday"
        android:layout_toLeftOf="@+id/addCoustmer_Birthday"
        android:layout_toStartOf="@+id/addCoustmer_Birthday" />

    <EditText
        android:id="@+id/addCoustmer_Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/addCoustmer_Birthday"
        android:layout_marginTop="12dp"
        android:layout_toRightOf="@+id/textView56"
        android:ems="10"
        android:background="@drawable/edit_text_search"
        android:hint="User ID"
        android:inputType="number"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:padding="12dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="User ID"
        android:layout_alignBaseline="@+id/addCoustmer_Id"
        android:layout_alignBottom="@+id/addCoustmer_Id"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/addCoustmer_Id"
        android:layout_toStartOf="@+id/addCoustmer_Id" />

    <EditText
        android:id="@+id/addCoustmer_gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/addCoustmer_Id"
        android:layout_alignStart="@+id/addCoustmer_Id"
        android:layout_below="@+id/addCoustmer_Id"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:background="@drawable/edit_text_search"
        android:hint="Geneder"
        android:inputType="textPersonName"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="12dp"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Gender"
        android:layout_alignBaseline="@+id/addCoustmer_gender"
        android:layout_alignBottom="@+id/addCoustmer_gender"
        android:layout_alignRight="@+id/textView57"
        android:layout_alignEnd="@+id/textView57"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/addCoustmer_job"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/addCoustmer_gender"
        android:layout_alignStart="@+id/addCoustmer_gender"
        android:layout_below="@+id/addCoustmer_gender"
        android:background="@drawable/edit_text_search"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Customer Job"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Job"
        android:padding="12dp"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignBaseline="@+id/addCoustmer_job"
        android:layout_alignBottom="@+id/addCoustmer_job"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/addCoustmer_job"
        android:layout_toStartOf="@+id/addCoustmer_job" />

    <EditText
        android:id="@+id/addCoustmer_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/addCoustmer_job"
        android:layout_alignStart="@+id/addCoustmer_job"
        android:layout_below="@+id/textView63"
        android:layout_marginTop="12dp"
        android:background="@drawable/edit_text_search"
        android:hint="Customer Email"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="12dp"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignBaseline="@+id/addCoustmer_email"
        android:layout_alignBottom="@+id/addCoustmer_email"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/addCoustmer_email"
        android:layout_toStartOf="@+id/addCoustmer_email" />

    <EditText
        android:id="@+id/addCoustmer_ETPhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/addCoustmer_email"
        android:layout_alignStart="@+id/addCoustmer_email"
        android:layout_below="@+id/textView64"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:hint="Customer Phone"
        android:inputType="phone"
        android:background="@drawable/edit_text_search"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="12dp"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Phone"
        android:layout_alignBottom="@+id/addCoustmer_ETPhoneNumber"
        android:layout_alignRight="@+id/textView58"
        android:layout_alignEnd="@+id/textView58"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/addCoustmer_Address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/addCoustmer_ETPhoneNumber"
        android:layout_alignStart="@+id/addCoustmer_ETPhoneNumber"
        android:layout_below="@+id/addCoustmer_ETPhoneNumber"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:background="@drawable/edit_text_search"
        android:inputType="textPersonName"
        android:hint="Customer Address"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="12dp"
        android:text="Address"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_alignBaseline="@+id/addCoustmer_Address"
        android:layout_alignBottom="@+id/addCoustmer_Address"
        android:layout_alignLeft="@+id/textView66"
        android:layout_alignStart="@+id/textView66"
        android:layout_toLeftOf="@+id/addCoustmer_Address"
        android:layout_toStartOf="@+id/addCoustmer_Address" />

    <Spinner
        android:id="@+id/SelectClubSpinner"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:textAlignment="center"
        android:gravity="center"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/addCoustmer_Address"
        android:layout_marginTop="21dp" />

    <TextView
        android:id="@+id/textView68"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add to Customer Club"
        android:padding="4dp"
        android:textColor="#000000"
        android:textSize="16dp"

        android:textStyle="bold"
        android:gravity="center"
        android:layout_toLeftOf="@+id/SelectClubSpinner"
        android:layout_toStartOf="@+id/SelectClubSpinner"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="6dp"
        android:layout_alignBottom="@+id/SelectClubSpinner" />

    <Spinner
        android:id="@+id/customer_spinner"
        android:layout_width="80dp"
        android:textAlignment="center"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView69"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:layout_alignBottom="@+id/textView68"
        android:layout_alignTop="@+id/textView68" />

    <TextView
        android:id="@+id/textView69"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Add to City"
        android:layout_alignLeft="@+id/addCoustmer_Address"
        android:layout_alignStart="@+id/addCoustmer_Address"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:layout_alignBottom="@+id/textView68"
        android:layout_alignTop="@+id/customer_spinner" />

    <Button
        android:id="@+id/add_Coustmer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/primaryDarkColor"
        android:text="Add Customer"
        android:textColor="#ffffff"
        android:textSize="16dp"
        android:textStyle="bold"
        tools:ignore="HardcodedText"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="59dp" />

</RelativeLayout>