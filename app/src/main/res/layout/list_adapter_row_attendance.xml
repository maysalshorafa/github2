<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:orientation="vertical"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:layout_width="0dp"
        android:layout_height="768dp"
        android:id="@+id/guideline"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="669dp"
        app:layout_constraintGuide_percent="0.6533203"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="768dp"
        android:id="@+id/guideline9"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="412dp"
        app:layout_constraintGuide_percent="0.40234375"/>

    <TextView
        android:text="TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/attendanceReportUser_TVH"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"/>

    <TextView
        android:text="TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/attendanceReportUser_TVEndTime"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/attendanceReportUser_TVDate"
        app:layout_constraintLeft_toLeftOf="@+id/guideline9"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="187dp"
        android:layout_height="24dp"
        android:id="@+id/attendanceReportUser_TVStartTime"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="@+id/attendanceReportUser_TVEndTime"
        app:layout_constraintRight_toLeftOf="@+id/guideline9"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline32"
        tools:layout_editor_absoluteY="6dp"
        android:layout_marginTop="0dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <android.support.constraint.Guideline
        android:layout_width="0dp"
        android:layout_height="768dp"
        android:id="@+id/guideline32"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.15039062"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="154dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/attendanceReportUser_TVDate"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline32"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintTop_toTopOf="@+id/attendanceReportUser_TVStartTime"
        android:textSize="18sp"/>

</android.support.constraint.ConstraintLayout>