        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/FE_version_no" />

    <EditTextPreference
        android:capitalize="sentences"
        android:defaultValue="@string/bo_version_no"
        android:enabled="false"
        android:inputType="textCapWords"
        android:key="LEAD_POS_RESULT_INTENT_BO_SETTING_ACTIVITY_BO_VERSION"
